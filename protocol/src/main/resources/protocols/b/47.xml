						<condition>
							<greater-than field="columns">0</greater-than>
						</condition>
						<field name="rows" type="byte" />
						<field name="x" type="byte" />
						<field name="z" type="byte" />
						<field name="data">
							<type name="array">
								<array-type>ubyte</array-type>
								<length-type>varint</length-type>
